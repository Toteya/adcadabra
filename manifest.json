{
  "name": "Ad-Cadabra",
  "description": "Intercepts ads and transform them into something cool.",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
      "128": "images/icon-128.png"
  }, 
  "action": {
    "default_icon": "icon-128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": []
    }
  ],
  "permissions": ["activeTab"]
}
